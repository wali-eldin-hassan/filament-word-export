{
  "name": "wali/filament-word-export",
  "description": "A powerful Filament plugin for exporting table data to Word documents with customizable headers, footers, and styling",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "filament",
    "laravel",
    "word",
    "docx",
    "export",
    "phpword",
    "table-export",
    "document-generation"
  ],
  "authors": [
    {
      "name": "Wali",
      "email": "creaspo6@gmail.com"
    }
  ],
  "homepage": "https://github.com/wali-eldin-hassan/filament-word-export",
  "support": {
    "issues": "https://github.com/wali-eldin-hassan/filament-word-export/issues",
    "source": "https://github.com/wali-eldin-hassan/filament-word-export"
  },
  "autoload": {
    "psr-4": {
          "Wali\\FilamentWordExport\\": "src/"
    },
    "files": [
            "src/helpers.php"
        ]
  },
  "require": {
    "php": "^8.3",
    "filament/filament": "^4.0",
    "spatie/laravel-package-tools": "^1.14",
    "phpoffice/phpword": "^1.4.0",
    "illuminate/support": "^11.0|^12.0"
  },
  "extra": {
    "laravel": {
      "providers": [
               "Wali\\FilamentWordExport\\Providers\\WordExportServiceProvider"
      ]
    }
  },
  "require-dev": {
    "laravel/pint": "^1.25",
    "rector/rector": "^2.1"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "lint": "pint",
    "lint:test": "pint --test",
    "lint:dirty": "pint --dirty",
    "refactor": "rector process",
    "refactor:dry": "rector process --dry-run",
    "format": [
      "@lint",
      "@refactor:dry"
    ],
    "fix": [
      "@lint",
      "@refactor"
    ]
  }
}
