{
  "name": "wali/filament-word-export",
  "description": "Filament plugin for exporting tables to Word (DOCX)",
  "autoload": {
    "psr-4": {
          "Wali\\FilamentWordExport\\": "src/"
    },
    "files": [
            "src/helpers.php"
        ]
  },
  "require": {
    "php": "^8.2",
    "filament/filament": "^4.0",
    "spatie/laravel-package-tools": "^1.14",
    "phpoffice/phpword": "^1.3"
  },
  "extra": {
    "laravel": {
      "providers": [
               "Wali\\FilamentWordExport\\Providers\\WordExportServiceProvider"
      ]
    }
  },
  "require-dev": {
    "laravel/pint": "^1.25",
    "rector/rector": "^2.1"
  },
  "scripts": {
    "lint": "pint",
    "lint:test": "pint --test",
    "lint:dirty": "pint --dirty",
    "refactor": "rector process",
    "refactor:dry": "rector process --dry-run",
    "format": [
      "@lint",
      "@refactor:dry"
    ],
    "fix": [
      "@lint",
      "@refactor"
    ]
  }
}
